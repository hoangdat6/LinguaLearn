#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dathv2004/Documents/BKDN/Learning/Python/EngLishProject/node_modules/.pnpm/react-scan@0.3.4_@types+react@19.1.5_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/react-scan/bin/node_modules:/home/dathv2004/Documents/BKDN/Learning/Python/EngLishProject/node_modules/.pnpm/react-scan@0.3.4_@types+react@19.1.5_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/react-scan/node_modules:/home/dathv2004/Documents/BKDN/Learning/Python/EngLishProject/node_modules/.pnpm/react-scan@0.3.4_@types+react@19.1.5_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules:/home/dathv2004/Documents/BKDN/Learning/Python/EngLishProject/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dathv2004/Documents/BKDN/Learning/Python/EngLishProject/node_modules/.pnpm/react-scan@0.3.4_@types+react@19.1.5_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/react-scan/bin/node_modules:/home/dathv2004/Documents/BKDN/Learning/Python/EngLishProject/node_modules/.pnpm/react-scan@0.3.4_@types+react@19.1.5_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/react-scan/node_modules:/home/dathv2004/Documents/BKDN/Learning/Python/EngLishProject/node_modules/.pnpm/react-scan@0.3.4_@types+react@19.1.5_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules:/home/dathv2004/Documents/BKDN/Learning/Python/EngLishProject/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
